  // url      = env("DATABASE_URL")
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = "sqlserver://localhost:1444;initial catalog=sample;user=sa;password=password123@;encrypt=false"
}



// file: prisma/schema.prisma

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
}